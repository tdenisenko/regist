<?xml version="1.0" encoding="utf-8"?>
<widget id="io.cordova.vuforiaplugin" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>regist</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
    <resource-file src="www/targets/StonesAndChips.xml" target="assets/StonesAndChips.xml" />
    <resource-file src="www/targets/StonesAndChips.dat" target="assets/StonesAndChips.dat" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <resource-file src="targets/StonesAndChips.xml" />
    <resource-file src="targets/StonesAndChips.dat" />
  </platform>
  <feature name="Device">
    <param name="ios-package" value="CDVDevice" />
  </feature>
  <plugin name="cordova-plugin-android-support-v4" version="21.0.1" />
  <plugin name="cordova-plugin-compat" version="1.0.0" />
  <plugin name="cordova-plugin-device" version="1.1.2" />
  <plugin name="cordova-plugin-file" version="4.2.0" />
  <plugin name="cordova-plugin-media" version="2.3.0" />
  <plugin name="cordova-plugin-vuforia" version="3.2.0" />
  <plugin name="cordova-plugin-vuforia-sdk" version="1.0.0" />
</widget>